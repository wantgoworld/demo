{
  "printWidth": 120,
  "proseWrap": "preserve",
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": false,
  "singleQuote": true,
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "bracketSameLine": true,
  "endOfLine": "lf",
  "overrides": [{ "files": "*.html", "options": { "parser": "html" } }]
}
